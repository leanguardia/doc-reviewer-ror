<% if current_user && current_user.estudiante? %>
    <%= render 'layouts/student' %><br>
<% end %>

<% if current_user && current_user.admin? %>
  <%= render 'layouts/admin' %><br>
<% end %>

<% if current_user && current_user.docente? %>
  <%= render 'layouts/professor' %><br>
<% end %>

<% if current_user && current_user.tutor? || current_user && current_user.relator? %>
  <%= render 'layouts/tutors' %><br>
<% end %>